{
  "version": 2,
  "alias": "koties-forum-ish",
  "name": "forum-ish",
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    }
  ],
  "build": {
    "env": {
      "REACT_APP_API_ENDPOINT": "https://damp-ocean-50804.herokuapp.com/api"
    }
  }
}
